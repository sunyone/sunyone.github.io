{
  "root": "./docs",
  "title" : "AWS",
  "author" : "AWS",
  "description" : "AWS",
  "language" : "zh-hans",
  "links" : {
      "sharing": {
          "google": false,
          "facebook": false,
          "twitter": false,
          "all": false
          }
      },
  "plugins": [
      "local-pagefooter",
      "expandable-menu",
      "-sharing",
      "sharing-plus",
      "splitter",
      "anchor-navigation-ex",
      "prism",
      "-highlight",
      "include-codeblock",
      "ace",
      "-lunr", "-search", "search-pro",
      "insert-logo-link-style",
      "mermaid-flow"
      ],
  "pluginsConfig": {
    "local-pagefooter": {
      "copyright":"Copyright &copy one 2014-2019",
      "islocal" : true,
      "modify_label": "该文件最后修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
      },
    "anchor-navigation-ex": {
      "showLevel": true,
      "associatedWithSummary": false,
      "printLog": false,
      "multipleH1": false,
      "mode": "float",
      "showGoTop":true,
      "float": {
        "floatIcon": "fa fa-navicon",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
        },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
        }
      },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "facebook", "google", "twitter",
        "weibo", "instapaper", "linkedin",
        "pocket", "stumbleupon"
        ]
      },
    "prism": {
      "css": ["prismjs/themes/prism-tomorrow.css"],
      "ignore": [
        "mermaid",
        "eval-js"
        ]
      },

    "include-codeblock": {
      "template": "default",
      "unindent": true,
      "theme": "monokai"
      },

    "insert-logo-link-style": {
      "src": "data:image/png;base64,",
      "style": "background: none;",
      "link": "https://sunyone.github.io/AWS/book/"
      }
  }
}
